{
    "name": "MyCademy/yii2-file-storage",
    "description": "File storage abstraction layer for Yii2",
    "keywords": ["yii2", "file", "storage", "abstraction", "amazon", "S3", "mongodb", "gridfs", "ssh", "sftp"],
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/MyCademy/yii2-file-storage/issues",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "https://github.com/MyCademy/yii2-file-storage/wiki",
        "source": "https://github.com/MyCademy/yii2-file-storage"
    },
    "authors": [
        {
            "name": "Paul Klimov",
            "email": "klimov.paul@gmail.com"
        },
        {
            "name": "MyCademy"
        }
    ],
    "require": {
        "yiisoft/yii2": "~2.0.13"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": { "yii2tech\\filestorage\\": "src" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
